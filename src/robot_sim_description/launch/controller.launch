<launch>

<rosparam file="$(find robot_sim_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="robot_sim" args="lf_hip_joint_position_controller rf_hip_joint_position_controller lh_hip_joint_position_controller rh_hip_joint_position_controller lf_upper_leg_joint_position_controller rf_upper_leg_joint_position_controller lh_upper_leg_joint_position_controller rh_upper_leg_joint_position_controller lf_lower_leg_joint_position_controller rf_lower_leg_joint_position_controller lh_lower_leg_joint_position_controller rh_lower_leg_joint_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/robot_sim/joint_states"/>
</node>

</launch>